  "private": true,
  "homepage": ".",
  "dependencies": {
    "@atlaskit/avatar": "^21.3.2",
    "@atlaskit/button": "^16.7.6",
    "@atlaskit/css-reset": "^6.0.1",
    "@atlaskit/icon": "^21.12.4",
    "@atlaskit/modal-dialog": "^12.5.6",
